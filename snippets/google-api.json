{
    "Create standard service": {
        "scope": "proto3",
        "prefix": [
            "svgapi",
            "svcrud"
        ],
        "body": [
            "// Generated according to https://cloud.google.com/apis/design/standard_methods",
            "service ${1:Type}Service {",
            "  rpc List${1}s(List${1}sRequest) returns (List${1}sResponse) {",
            "    option (google.api.http) = {",
            "      get: \"/v${2:1alpha1}/${1/(.*)/${1:/downcase}/}s\"",
            "    };",
            "  }",
            "",
            "  rpc Get${1}(Get${1}Request) returns (${1}) {",
            "    option (google.api.http) = {",
            "      get: \"/v${2:1alpha1}/{name=${1/(.*)/${1:/downcase}/}s/*}\"",
            "    };",
            "  }",
            "",
            "  rpc Create${1}(Create${1}Request) returns (${1}) {",
            "    option (google.api.http) = {",
            "      post: \"/v${2:1alpha1}/${1/(.*)/${1:/downcase}/}s\"",
            "      body: \"${1/(.*)/${1:/downcase}/}\"",
            "    };",
            "  }",
            "",
            "  rpc Update${1}(Update${1}Request) returns (${1}) {",
            "    option (google.api.http) = {",
            "      patch: \"/v${2:1alpha1}/{${1/(.*)/${1:/downcase}/}.name=${1/(.*)/${1:/downcase}/}s/*}\"",
            "      body: \"${1/(.*)/${1:/downcase}/}\"",
            "    };",
            "  }",
            "",
            "  rpc Delete${1}(Delete${1}Request) returns (google.protobuf.Empty) {",
            "    option (google.api.http) = {",
            "      delete: \"/v${2:1alpha1}/{name=${1/(.*)/${1:/downcase}/}s/*}\"",
            "    };",
            "  }",
            "}",
            "",
            "// message ${1} {",
            "// ! This should be defined elsewere",
            "// }",
            "",
            "message List${1}sRequest {",
            "  // The parent resource name, for example, \"shelves/shelf1\"",
            "  string parent = 1;",
            "",
            "  // The maximum number of items to return.",
            "  int32 page_size = 2;",
            "",
            "  // The next_page_token value returned from a previous List request, if any.",
            "  string page_token = 3;",
            "}",
            "",
            "message List${1}sResponse {",
            "  // The field name should match the noun \"${1/(.*)/${1:/downcase}/}\" in the method name.",
            "  // There will be a maximum number of items returned based on the page_size field in the request.",
            "  repeated ${1} ${1/(.*)/${1:/downcase}/}s = 1;",
            "",
            "  // Token to retrieve the next page of results, or empty if there are no more results in the list.",
            "  string next_page_token = 2;",
            "}",
            "",
            "message Get${1}Request {",
            "  // The field will contain name of the resource requested.",
            "  string name = 1;",
            "}",
            "",
            "message Create${1}Request {",
            "  // The parent resource name where the ${1/(.*)/${1:/downcase}/} is to be created.",
            "  string parent = 1;",
            "",
            "  // The ${1/(.*)/${1:/downcase}/} id to use for this ${1/(.*)/${1:/downcase}/}.",
            "  string ${1/(.*)/${1:/downcase}/}_id = 2;",
            "",
            "  // The ${1/(.*)/${1:/downcase}/} resource to create.",
            "  // The field name should match the Noun in the method name.",
            "  ${1} ${1/(.*)/${1:/downcase}/} = 3;",
            "}",
            "",
            "message Update${1}Request {",
            "  // The ${1/(.*)/${1:/downcase}/} resource which replaces the resource on the server.",
            "  ${1} ${1/(.*)/${1:/downcase}/} = 1;",
            "",
            "  // The update mask applies to the resource. For the `FieldMask` definition,",
            "  // see https://developers.google.com/protocol-buffers/docs/reference/google.protobuf#fieldmask",
            "  FieldMask update_mask = 2;",
            "}",
            "",
            "message Delete${1}Request {",
            "  // The resource name of the ${1/(.*)/${1:/downcase}/} to be deleted.",
            "  string name = 1;",
            "}",
            ""
        ],
        "description": "Create a standard service according to https://cloud.google.com/apis/design/standard_methods"
    },
    "option google.api.http.get": {
        "scope": "proto3",
        "prefix": [
            "optget"
        ],
        "body": [
            "option (google.api.http) = {",
            "   get: \"${1:/v0alpha1/resources}\"",
            "};"
        ],
        "description": "annotating google.api.http GET method"
    },
    "option google.api.http.post": {
        "scope": "proto3",
        "prefix": [
            "optpost"
        ],
        "body": [
            "option (google.api.http) = {",
            "   post: \"${1:/v0alpha1/resources}\"",
            "   body: \"*\"",
            "};"
        ],
        "description": "annotating google.api.http POST method"
    },
    "option google.api.http.patch": {
        "scope": "proto3",
        "prefix": [
            "optpatch",
            "optpth"
        ],
        "body": [
            "option (google.api.http) = {",
            "   patch: \"${1:/v0alpha1/resources}\"",
            "   body: \"*\"",
            "};"
        ],
        "description": "annotating google.api.http PATCH method"
    },
    "option google.api.http.delete": {
        "scope": "proto3",
        "prefix": [
            "optdel"
        ],
        "body": [
            "option (google.api.http) = {",
            "   delete: \"${1:/v0alpha1/resources}\"",
            "};"
        ],
        "description": "annotating google.api.http DELETE method"
    }
}